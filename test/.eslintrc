{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "consistent-return": 0,
    "class-methods-use-this": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-dynamic-require": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "no-param-reassign": 0,
    "global-require": 0,
    "no-console": 0
  }
}
